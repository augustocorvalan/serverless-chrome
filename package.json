{
  "name": "app-dibs-lighthouse",
  "version": "1.0.0-34",
  "description": "Run headless Chrome, serverless-ly",
  "main": "handler.js",
  "engines": {
    "node": ">= 6.10.0"
  },
  "scripts": {
    "ava": "ava",
    "coverage": "nyc report",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact",
    "release": "scripts/release.sh",
    "postversion": "scripts/sync-package-versions.sh"
  },
  "keywords": [
    "serverless",
    "chrome",
    "chromium",
    "headless",
    "aws",
    "lambda",
    "serverless-framework"
  ],
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lighthouse": "^2.8.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "ava": "0.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "codacy-coverage": "2.0.3",
    "coveralls": "3.0.0",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "nyc": "11.3.0",
    "tap-xunit": "1.7.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "sourceMaps": "inline",
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.10"
          }
        }
      ],
      "stage-3"
    ]
  }
}
